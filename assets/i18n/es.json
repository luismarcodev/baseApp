            "item3": {
              "title": "Séneca el Joven",
              "subtitle": "4 a.C. - 65 d.C.",
              "description": "Séneca fue un filósofo estoico romano, estadista y dramaturgo que creía en la importancia de la ética personal y la paz interior, y escribió extensamente sobre temas de felicidad y virtud.",
              "imageUrl": "https://upload.wikimedia.org/wikipedia/commons/thumb/5/5e/Seneca_the_Younger.jpg/1200px-Seneca_the_Younger.jpg"
            },
            "item4": {
              "title": "Crisipo",
              "subtitle": "280 - 207 a.C.",
              "description": "Crisipo fue un filósofo estoico griego que hizo contribuciones significativas a la filosofía estoica, incluyendo la idea de que la razón era la fuerza rectora en el universo, y que los seres humanos podían usar la razón para alcanzar una vida virtuosa.",
              "imageUrl": "https://upload.wikimedia.org/wikipedia/commons/6/62/Chrysippus_2.jpg"
            },
            "item5": {
              "title": "Zenón de Citio",
              "subtitle": "334 - 262 a.C.",
              "description": "Zenón de Citio fue el fundador de la filosofía estoica, y enseñó que la virtud y la sabiduría eran las claves para una vida feliz, y que las circunstancias externas eran en gran medida irrelevantes.",
              "imageUrl": "https://upload.wikimedia.org/wikipedia/commons/7/77/Zeno_of_Citium.jpg"
            }
          }
        }
      }
